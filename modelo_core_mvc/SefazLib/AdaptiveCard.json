{
  "type": "AdaptiveCard",
  "body": [
    {
      "type": "FactSet",
      "facts": [
        {
          "title": "Nome",
          "value": "${nome}"
        },
        {
          "title": "Nome completo",
          "value": "${nomecompleto}"
        },
        {
          "title": "Cargo",
          "value": "${cargo}"
        },
        {
          "title": "Login",
          "value": "${login}"
        },
        {
          "title": "email",
          "value": "${email}"
        },
        {
          "title": "id",
          "value": "${id}"
        }
      ]
    }
  ],
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.0"
}