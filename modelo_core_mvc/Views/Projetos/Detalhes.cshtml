@using modelo_core_mvc.projetos;
@model modelo_core_mvc.projetos.ProjetosModel

<h1>@ViewData["Message"]</h1>

<div>
    <h4>Projeto: @Html.DisplayFor(model => model.nome)</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.nome)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.nome)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.descricao)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.descricao)
        </dd>
    </dl>
</div>
<partial name="_Botoes" model='new Tuple<string, string, string, string>("", "", "Projetos/Alterar/"+@Model.id, "Cancelar")' />

