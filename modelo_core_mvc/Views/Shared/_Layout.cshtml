</head>

<body style="zoom: @valorZoom;">
    <script src="~/js/acessibilidade.js?v=@DateTime.Now.Ticks"></script>
    <script>
        window.addEventListener('DOMContentLoaded', function () {
            contrastState = @contrastState;
            valorMudanca = @valorMudanca;
            alterarZoom();
        });
    </script>

    <div id="aplicacao">
        <header id="cabecalho_mobile" class="painel_cabecalho">
            <partial name="_Cabecalho" />
        </header>

        <div id="painel_aplicacao">
            <section id="painel_lateral" class="oculto @(estaRecolhido ? "recolhido" : "")">
                <div id="brasaoPainel">
                    <div id="fundoBrasaoDesktop" class="fundoBrasao">
                        <img class="brasao" src="https://devbsacdn.blob.core.windows.net/v01/img/BRASAO_GOV_SP_Original.svg" alt="Brasão do Estado de São Paulo">
                    </div>
                    <span id="orgao">Secretaria da Fazenda e Planejamento</span>
                </div>
                <button id="botao_expandir"
                        type="button"
                        title="Botão para expandir ou recolher o menu (<alt c>)"
                        aria-label="Botão para expandir ou recolher o menu"
                        accesskey="c"
                        onclick="alternarClasse(['painel_lateral', 'icone_expandir', 'icone_recolher'],'recolhido')">
                    <span id="icone_expandir" class="@(!estaRecolhido ? "recolhido" : "")">
                        <i class="fas fa-angle-right"></i>
                    </span>
                    <span id="icone_recolher" class="@(estaRecolhido ? "recolhido" : "")">
                        <i class="fas fa-angle-left"></i>
                    </span>
                </button>

                <div id="menu_lateral" onclick="alternarClasse(['painel_lateral', 'painel_principal'],'oculto',false)">
                    <partial name="_ItensMenu" />
                </div>
            </section>

            <section id="painel_principal" class="@(estaRecolhido ? "recolhido" : "")">
                <header id="cabecalho-desktop" class="painel_cabecalho">
                    <partial name="_Cabecalho" />
                    <nav id="menu_popup" class="oculto">
                        <partial name="_ItensMenu" />
                    </nav>
                </header>

                <div id="painel_conteudo">
                    <partial name="_BreadCrumb" />

                    <main id="conteudo" class="shadow" tabindex="0">
                        @RenderBody()
                    </main>
                </div>

                <div id="topoDaPagina" title="Voltar ao topo">
                    <a href="#">Topo da página</a>
                </div>

                <div id="mensagem_erro">
                    @if (ViewData["Erro"] != null)
                    {
                        @ViewData["Erro"]
                    }
                </div>
            </section>
        </div>
    </div>

    <partial name="_Aguarde" />
    <partial name="_Scripts" />

    @RenderSection("Scripts", required: false)
</body>

</html>
